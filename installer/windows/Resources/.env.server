# Maintainerr Environment Configuration
# This file will be copied to the data directory during installation

# The root directory where Maintainerr is installed
APP_DIR=

# Where Maintainerr will store its data
DATA_DIR=

# API Port (default: 3001)
API_PORT=3001

# UI Port (default: 6246)
UI_PORT=6246

# UI Hostname (default: 0.0.0.0)
UI_HOSTNAME=0.0.0.0

# Node environment
NODE_ENV=production

# Version tag
VERSION_TAG=stable

# Git SHA (populated during build)
GIT_SHA=
